
# type of database. Run with -dbhelp for details
schemaspy.t=pgsql11
# optional path to alternative jdbc drivers.
schemaspy.dp=./postgresql-42.2.16.jar
# database properties: host, port number, name, user, password
schemaspy.host=0.0.0.0
schemaspy.port=5432
schemaspy.db=postgres
schemaspy.u=postgres
schemaspy.p=somepassword
# output dir to save generated files
schemaspy.o=./output
# db schema to generate diagrams for
schemaspy.s=public
# image output, either svg or default is png
schemaspy.imageformat=svg